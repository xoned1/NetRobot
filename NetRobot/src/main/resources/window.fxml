<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.canvas.Canvas?>

<GridPane xmlns:fx="http://javafx.com/fxml" xmlns="http://javafx.com/javafx/8"
	fx:controller="de.timo.netrobot.ui.WindowController" alignment="center"
	hgap="10" vgap="10">
	
	
		<Button fx:id="btnStartServer" onAction="#btnStartServer_onclick"
			GridPane.columnIndex="0" GridPane.rowIndex="0">Start Server..</Button>
		<Canvas fx:id="canvasConStatus" height="20" width="25" 
			GridPane.columnIndex="1" GridPane.rowIndex="0" />
			
			
		<padding>
			<Insets top="25" right="25" bottom="10" left="25" />
		</padding>
</GridPane>